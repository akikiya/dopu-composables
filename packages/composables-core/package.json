{
  "name": "@dopu/composables-core",
  "description": "A Collection of Vue3 Composables",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/composables-core.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "import": "./dist/composables-core.js",
    "require": "./dist/composables-core.cjs"
  },
  "scripts": {
    "dev:docs": "serve ./docs",
    "collect:modules": "pnpm -F=scripts run collect",
    "build:docs": "pnpm run collect:modules && typedoc ./lib/index.ts",
    "build:lib": "pnpm run collect:modules && vite build && tsc",
    "build": "pnpm run build:lib && pnpm run build:docs",
    "publish": "pnpm build && pnpm publish --access public --registry https://registry.npmjs.org/ --no-git-checks"
  },
  "dependencies": {
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "serve": "^14.2.3",
    "typedoc": "^0.26.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  }
}
